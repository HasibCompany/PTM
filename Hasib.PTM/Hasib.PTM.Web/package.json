    "pre-refresh": "rd /q /s \"src/app/shared\" 2>nul & rd /q /s \"src/app/start\" 2>nul & rd /q /s \"src/app/core\" 2>nul &  rd /q /s \"src/app/theme\" 2>nul & rd /q /s \"src/app/ui\" 2>nul & rd /q /s \"src/app/ui-custom\" 2>nul & rd /q /s \"src/app/commonBusiness\" 2>nul",
